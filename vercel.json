{
  "rewrites": [
    {
      "source": "/((?!static|favicon|manifest|api|_next/static|app/static|assets|images|css|js|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot).*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}