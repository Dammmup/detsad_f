{
  "rewrites": [
    {
      "source": "/((?!static|favicon.ico|manifest.json|logo192.png|logo512.png).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}